	<queryString>
		<![CDATA[select
  j.doc_returnf_journal as  journal,
  p.name as product,
  d.nomer as docname,
  d.dates,
  a.name as agent,
  'Без НДС' as nds,
  sp.price,
  sp.price*j.qty as summa,
  j.qty  as ves,
  (select CURR_STR from currencystr((select sum(sp.price*j.qty) from doc_returnf_journal j inner join scladproduct sp on j.scladproduct = sp.scladproduct where j.doc_returnf = $P{DOC}),'RU',1)) as strname,
  (select val_str from setup where setup = 6) as firma,
  (select val_str from setup where setup = 7) as firmainn,
  (select val_str from setup where setup = 8) as direktor,
  c.name as color
from doc_returnf_journal j
inner join doc_returnf d on d.doc_returnf = j.doc_returnf
inner join scladproduct sp on sp.scladproduct = j.scladproduct
inner join product p on p.product = sp.product
inner join agent a on a.agent = d.agent
inner join color c on c.color = sp.color
where d.doc_returnf =  $P{DOC}]]>
	</queryString>
