	<queryString>
		<![CDATA[select
  d.u_document,
  d.dates,
  o.name,
  DECODE(o.pm, 1, d.summa, 2, -d.summa) as summas,
  d.texts
from u_document d
inner join u_operation o on o.u_operation = d.u_operation
where d.dates between $P{date1} and $P{date2}]]>
