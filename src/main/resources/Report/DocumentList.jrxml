	<parameter name="date1" class="java.util.Date"/>
	<parameter name="date2" class="java.util.Date"/>
	<parameter name="agent" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  d.document,
  a.name as agent,
  d.nomer,
  d.dates,
  coalesce((select sum(kj.price* (decode(d.vid,2,kj.width,kj.ves))) from documentjournal kj where kj.document = d.document and kj.del = 0),0) as summa,
  coalesce((select sum(kj.ves) from documentjournal kj where kj.document = d.document and kj.del = 0),0) as ves
from document d
inner join agent a on a.agent = d.agent
where d.vid = 1 and d.dates between $P{date1} and $P{date2} and d.status > 0
and d.agent  = decode($P{agent}, 0, d.agent, $P{agent})
order by a.name, d.dates]]>
	</queryString>
