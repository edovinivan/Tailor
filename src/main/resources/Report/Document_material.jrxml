	<parameter name="DOC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  d.nomer,
  d.dates,
  a.name as agent,
  c.name as color ,
  p.name as product,
  d.vid,
  dj.price,
  dj.light,
  dj.ves,
  dj.width,
  dj.documentjournal,
  dj.ves*dj.price as summas
from documentjournal dj
inner join document d on d.document = dj.document
inner join product p on p.product = dj.product
inner join color c on c.color = dj.color
inner join agent a on a.agent = d.agent
where d.document = $P{DOC} and dj.del = 0 order by p.name]]>
	</queryString>
