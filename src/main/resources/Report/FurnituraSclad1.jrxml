	<parameter name="sclad" class="java.lang.Integer"/>
	<parameter name="pgruppa" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  pg.name as pg,
  p.name as product,
  c.name as color,
  sc.width as qty,
  s.name as scladname
from scladproduct sc
inner join product p on p.product = sc.product
inner join color c on c.color = sc.color
inner join productgruppa pg on pg.productgruppa = p.productgruppa
inner join sclad s on s.sclad = sc.sclad
where sc.vid = 2 and sc.width > 0 and s.sclad = decode($P{sclad},0,s.sclad, $P{sclad})
and pg.productgruppa = decode($P{pgruppa},0,pg.productgruppa, $P{pgruppa})
order by pg, s.name, product, color]]>
	</queryString>
