	<parameter name="chex" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  m.nomer as marsh,
  z.nomer as zadacha,
  (select sum(zkr.qty) from zadachakomplektovkarazmeri zkr where zkr.zadachakomplektovka = m.zadachakomplektovka) as qty,
  ( select
         list('(' || r.name || '-' || zkr.qty || ')')
     from zadachakomplektovkarazmeri zkr
     inner join razmeri r on r.razmeri = zkr.razmeri
     where zkr.zadachakomplektovka = m.zadachakomplektovka  and zkr.qty > 0
  ) as razm,
  m.datasend as dates,
  rg.name as chexname,
  md.article,
  c.name as color
from zadachakomplektovka m
inner join zadacha z on m.zadacha = z.zadacha
inner join model md on md.model = z.model
inner join rabotnikgruppa rg on rg.rabotnikgruppa = m.chex
inner join color c on c.color = m.color
where m.chex = (case $P{chex} when 0 then m.chex else $P{chex} end)   and m.status between 3 and 4
order by  md.article,chexname, dates, zadacha, marsh]]>
	</queryString>
