	<parameter name="DOC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  o.name as operation,
  rab.name as rab,
  ro.dates,
  ro.qty,
  m.article,
  zk.nomer
from zadachakomplektovka zk
inner join zadacha z on z.zadacha = zk.zadacha
inner join model m on m.model = z.model
inner join RABOTNIKOPERATION ro on ro.zadachakomplektovka = zk.zadachakomplektovka
inner join operation o on o.operation = ro.operation
inner join rabotnik rab on rab.rabotnik = ro.rabotnik
where zk.zadachakomplektovka = $P{DOC}
order by o.name]]>
	</queryString>
