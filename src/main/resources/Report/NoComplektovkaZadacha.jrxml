	<parameter name="date1" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
  z2.zadacha,
  z2.dates,
  z2.nomer,
  z2.article,
  z2.cou,
  z2.dm,
  (select count(zn2.zadachanastil) from zadachanastil zn2 where zn2.zadacha = z2.zadacha ) as zkn,
  (select sum(zz.qty) from ZADACHARAZMERI zz where zz.zadacha = z2.zadacha) as qty
from (

select
  zadacha,
  dates,
  nomer,
  article,
  sum(cou) as cou,
  max(dm) as dm

 from (
select
 z.zadacha,
 z.dates,
 z.nomer,
 m.article,
 (select count(zk.zadachakomplektovka) from zadachakomplektovka zk where zk.zadacha = z.zadacha) as cou,
 (select max(zn1.dateclose) from zadachanastil zn1 where zn1.zadacha = z.zadacha ) as dm

from zadacha z
inner join zadachanastil zn on zn.zadacha = z.zadacha
inner join model m on m.model = z.model
where z.del = 0 and zn.status > 1 and z.dates >= $P{date1})
group by  zadacha, dates, nomer, article) z2 where cou =0
order by 2]]>
	</queryString>
