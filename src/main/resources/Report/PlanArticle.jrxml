	<queryString>
		<![CDATA[
select product, color, sum(qty) as qty, list(article) as article
from (

select
 pr.name as product,
 c.name as color,
 mp.qty_start * pa.qty as qty,
 m.article as article
from planarticle pa
inner join model m on m.model = pa.model
inner join modelproduct mp on mp.model = m.model and mp.vid = 1 and mp.tipproduct = 1
inner join product pr on pr.product = mp.product
inner join color c on c.color = mp.color
where  pa.dates between $P{date1} and $P{date2}
union
select
 pr.name as product,
 c.name as color,
 mp.qty_start * pa.qty as qty,
 m.article
from planarticle pa
inner join model m on m.model = pa.model
inner join modelproduct mp on mp.model = m.model and mp.vid = 1 and mp.tipproduct != 1
inner join product pr on pr.product = mp.product
inner join color c on c.color = mp.color
where  pa.dates between $P{date1} and $P{date2})
group by product, color
order by 1, 2]]>
	</queryString>
