	<subDataset name="ZKFurn" uuid="e947ba6f-676e-4177-a154-4e2cbbb21615">
		<parameter name="zk" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
  p.name as product,
  c.name as color,
  zf.qty
from zadachafurnitura zf
inner join zadachakomplektovka zk on zk.zadachakomplektovka = zf.zadachakomplektovka
inner join scladproduct sp on sp.scladproduct = zf.scladproduct
inner join product p on sp.product = p.product
inner join color c on c.color = sp.color
where zk.zadachakomplektovka = $P{zk}]]>
		</queryString>
		<field name="PRODUCT" class="java.lang.String"/>
		<field name="COLOR" class="java.lang.String"/>
		<field name="QTY" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[РєРѕР»РёС‡РµСЃС‚РІРѕ]]></fieldDescription>
		</field>
	</subDataset>
	<parameter name="DOC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
 z.nomer as zadacha,
 r.name as rabotnik,
 zk.nomer as zkomnomer,
 zk.dates,
 c.name as color,
 (select list(k.name || '('||mk.qty||')','; ') from modelkteevaya mk inner join kleevaya k on k.kleevaya = mk.kleevaya where mk.model = z.model) as kleevaya,
 (select list(k.name,'; ') from modelprintdetali mk inner join printdetali k on k.printdetali = mk.printdetali where mk.vid=1 and mk.model = z.model) as printdetali,
 (select list(k.name,'; ') from modelprintdetali mk inner join printdetali k on k.printdetali = mk.printdetali where mk.vid=2 and mk.model =  z.model) as vihivka,
 (select list(k.name || '(' || mk.qty || ')','; ') from modelsablon mk inner join sablon k on k.sablon = mk.sablon where mk.model =  z.model) as sablon,
 rr.name as razmer,
 zkr.qty,
 m.name as model,
 m.article as article,
 zpr.qty as qty1,
 zpr.vopros,
 zpr.otvet,
 zp.date1
from zkpretenzia zp
inner join zadachakomplektovka zk on zk.zadachakomplektovka = zp.zadachakomplektovka
inner join zadacha z on zk.zadacha = z.zadacha
inner join rabotnik r on r.rabotnik = zk.rabotnik
inner join color c on zk.color = c.color
inner join zadachakomplektovkarazmeri zkr on zkr.zadachakomplektovka = zk.zadachakomplektovka
inner join zkpretenziarazmer zpr on zpr.zkrazmer = zkr.zadachakomplektovkarazmeri
inner join zadacharazmeri zr on zr.zadacharazmeri = zkr.zadacharazmeri
inner join razmeri rr on rr.razmeri = zr.razmeri
inner join model m on m.model = z.model
where zp.zkpretenzia = $P{DOC} and zkr.qty > 0]]>
	</queryString>
