	<queryString>
		<![CDATA[select
 z.nomer,
 z.datesclad,
 p.name as product,
 c.name as color,
 sp.ves,
 'Осн'as tip
from zadacha z
inner join zadachascladproduct zsp on zsp.zadacha = z.zadacha
inner join scladproduct sp on sp.scladproduct = zsp.scladproduct
inner join product p on p.product = sp.product
inner join color c on c.color = sp.color
where z.datesclad between $P{date1} and $P{date2}
  and z.status >=2 and sp.vid = 1

union

select
 z.nomer,
 z.datesclad,
 p.name as product,
 c.name as color,
 sp.ves,
 'Доп' as tip
from zadacha z
inner join zadachadopscladproduct zsp on zsp.zadacha = z.zadacha
inner join scladproduct sp on sp.scladproduct = zsp.scladproduct
inner join product p on p.product = sp.product
inner join color c on c.color = sp.color
where z.datesclad between $P{date1} and $P{date2}
  and z.status >=2 and sp.vid = 1

order by 2,1]]>
	</queryString>
