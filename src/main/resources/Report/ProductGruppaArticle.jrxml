	<parameter name="DOC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  m.article,
  m.name,
  mp.qty_start,
  pg.name as pgname
from product p
inner join productgruppa pg on pg.productgruppa = p.productgruppa
inner join modelproduct mp on mp.product = p.product
inner join model m on m.model = mp.model
where pg.productgruppa = $P{DOC} and mp.tipproduct = 1
order by m.name]]>
	</queryString>
