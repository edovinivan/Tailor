	<parameter name="date1" class="java.util.Date"/>
	<parameter name="date2" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
 sp.passport,
 p.name as product,
 c.name as color,
 sp.ves,
 z.nomer,
 z.datesclad
from zadachascladproduct zsp
inner join zadacha z on zsp.zadacha = z.zadacha
inner join scladproduct sp on zsp.scladproduct = sp.scladproduct
inner join product p on p.product = sp.product
inner join color c on sp.color = c.color
where z.datesclad between $P{date1} and $P{date2} and z.status >= 2 and zsp.nastil = 0
union
select
 sp.passport,
 p.name as product,
 c.name as color,
 sp.ves,
 z.nomer,
 z.datesclad
from zadachascladproduct zsp
inner join zadacha z on zsp.zadacha = z.zadacha
inner join zadachanastil zn on zn.zadacha = z.zadacha
inner join scladproduct sp on zsp.scladproduct = sp.scladproduct
inner join product p on p.product = sp.product
inner join color c on sp.color = c.color
where z.datesclad between $P{date1} and $P{date2} and zn.dateclose  > $P{date2} and zsp.nastil = 1
order by 6]]>
	</queryString>
