	<queryString>
		<![CDATA[select
  rj.reviziajournal,
  r.nomer,
  r.dates,
  sp.passport,
  p.name as product,
  c.name as color,
  rj.ves,
  rj.vesdb,
  rj.vesdb-rj.ves as rash,
  s.name as sclad
from reviziajournal rj
inner join scladproduct sp on sp.scladproduct = rj.scladproduct
inner join revizia r on r.revizia = rj.revizia
inner join product p on p.product = sp.product
inner join color c on sp.color = c.color
inner join sclad s on s.sclad = sp.sclad
where r.revizia = $P{DOC}]]>
	</queryString>
