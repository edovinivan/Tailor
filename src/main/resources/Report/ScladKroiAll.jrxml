	<queryString>
		<![CDATA[select
  zk.zadachakomplektovka,
  zk.nomer,
  m.article,
  zk.dates,
  r.name,
  coalesce((select sum(z.qty) from zadachakomplektovkarazmeri z where z.zadachakomplektovka = zk.zadachakomplektovka),0) as cou,
  c.name as color

from zadachakomplektovka zk
inner join rabotnikgruppa rg on rg.rabotnikgruppa = zk.chex
inner join model m on m.model = zk.model
inner join color c on zk.color = c.color
inner join rabotnik r on r.rabotnik = zk.rabotnik
where zk.status =11]]>
	</queryString>
