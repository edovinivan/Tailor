	<parameter name="date1" class="java.util.Date"/>
	<parameter name="date2" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
  r.name as rabname,
  zn.nomer as znastil,
  z.nomer as zadacha,
  zn.dateclose,
  zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle as vesnast,
  (case when (zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle)=0 then 0 else ((zn.qtyend+zn.qtydel+zn.qtylosk+zn.qtytruba)/((zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle)/100.0 )) end) as proc,
  coalesce((select sum(zn1.qtyend+zn1.qtydel+zn1.qtygut+zn1.qtylosk+zn1.qtytruba+zn1.qtyaddarticle) from zadachanastil zn1 where zn1.zadacha = z.zadacha and zn1.status>1),0) as allzadachaqty,
zn.qtynull

from zadachanastil zn
inner join zadacha z on zn.zadacha = z.zadacha
inner join rabotnik r on r.rabotnik = zn.nastil
where zn.dateclose between $P{date1} and $P{date2} and zn.status > 1 and r.rabotnikgruppa = 4
union
select
  r.name,
  zn.nomer,
  z.nomer,
  zn.dateclose,
  zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle as vesnast,
 (case when (zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle)=0 then 0 else ((zn.qtyend+zn.qtydel+zn.qtylosk+zn.qtytruba)/((zn.qtyend+zn.qtydel+zn.qtygut+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle)/100.0 )) end) as proc,
  coalesce((select sum(zn1.qtyend+zn1.qtydel+zn1.qtygut+zn1.qtylosk+zn1.qtytruba+zn1.qtyaddarticle) from zadachanastil zn1 where zn1.zadacha = z.zadacha and zn1.status>1),0) as allzadachaqty,
zn.qtynull
from zadachanastil zn
inner join zadacha z on zn.zadacha = z.zadacha
inner join rabotnik r on r.rabotnik = zn.rezak
where zn.dateclose between $P{date1} and $P{date2} and zn.status > 1 and r.rabotnikgruppa = 5
order by 1,4,3,2]]>
	</queryString>
