	<parameter name="date2" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
  (case m.price when 0 then (z.koef*tm.koef*(select sum(mp1.qty) from modelproduct mp1 where mp1.model = m.model and mp1.vid = 1)) else m.price end) as price1d,
  (select sum(zr1.qty) from zadachakomplektovkarazmeri zr1 where zr1.zadachakomplektovka = zk.zadachakomplektovka) as allqty,
  r.name,
  zk.dates,
  m.article,
  zk.nomer,
  (case $P{rab} when 0 then 'Все' else (select name from rabotnik where rabotnik = $P{rab}) end) as rab,
  (select sum(productprice * productves)/max(qty3) from GET_ZADACHA_FOR_PRINT(z.zadacha)) as price1,
 z.nomer as zadachaname
from zadachakomplektovka zk
inner join zadacha z on z.zadacha = zk.zadacha
inner join rabotnik r on r.rabotnik = zk.rabotnik
inner join model m on m.model = z.model
inner join tipmodel tm on tm.tipmodel = m.tipmodel
where zk.dates between $P{date1} and $P{date2}
and zk.rabotnik = (case $P{rab} when 0 then zk.rabotnik else $P{rab} end)
and zk.status > 0
order by r.name,zk.dates]]>
	</queryString>
