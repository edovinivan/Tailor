	<subDataset name="RabOper" uuid="6783cf42-4538-4ca9-8fa6-14fffcbd66a3">
		<parameter name="zd" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
 zk.nomer,
 coalesce(sum(ro.summaall), 0) as summ
from zadachakomplektovka zk
left join rabotnikoperation ro on zk.zadachakomplektovka = ro.zadachakomplektovka
where zk.zadacha = $P{zd}
group by zk.nomer]]>
		</queryString>
		<field name="NOMER" class="java.lang.String">
			<fieldDescription><![CDATA[РЅРѕРјРµСЂ РєРѕРјРїР»РµРєС‚РѕРІРєРё]]></fieldDescription>
		</field>
		<field name="SUMM" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="ZN" uuid="30fd7cbb-18fc-458a-b5c5-8081540c9451">
		<parameter name="zn" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
  t.name,
  (zn.qtygut) as qgut,
  (zn.qtyend+zn.qtydel+zn.qtylosk+zn.qtytruba) as qdel,
  (zn.qtyaddarticle) as qa,
   zn.qtynull as qtynull,
  (zn.qtyend+zn.qtydel+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle+zn.qtygut+zn.qtynull) as qall
from zadachanastil zn
inner join tipnastila t on t.tipnastila = zn.tipnastila
where zn.zadacha = $P{zn}
order by t.name
/*group by t.name*/]]>
		</queryString>
		<field name="NAME" class="java.lang.String"/>
		<field name="QGUT" class="java.math.BigDecimal"/>
		<field name="QDEL" class="java.math.BigDecimal"/>
		<field name="QA" class="java.math.BigDecimal"/>
		<field name="QTYNULL" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[РћСЃС‚Р°С‚РѕС‡РЅС‹Р№ РІРµСЃ]]></fieldDescription>
		</field>
		<field name="QALL" class="java.math.BigDecimal"/>
		<variable name="sumqgut" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QGUT}]]></variableExpression>
		</variable>
		<variable name="allqty" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QALL}]]></variableExpression>
		</variable>
		<variable name="sumqdel" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QDEL}]]></variableExpression>
		</variable>
		<variable name="sumqa" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QA}]]></variableExpression>
		</variable>
		<variable name="sumqtynull" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QTYNULL}]]></variableExpression>
		</variable>
	</subDataset>
