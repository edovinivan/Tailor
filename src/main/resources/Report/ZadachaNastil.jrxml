	<parameter name="DOC" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[40]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  zn.nomer,
  tn.name as tipnastila,
  tna.name as tipnastilania,
  zn.plusdlina,
  zn.addarticle,
  zn.dates,
  sp.passport,
  p.name as product,
  c.name as color,
  sp.ves,
  sp.width,
  m.article,
  zn.dlina,
  m.name as modelname,
  zn.texts,
  zn.beika

from zadachanastil zn
inner join tipnastila tn on tn.tipnastila = zn.tipnastila
inner join tipnastilania tna on tna.tipnastilania = zn.tipnastilania
inner join zadachanastilproduct zp on zp.zadachanastil = zn.zadachanastil
inner join zadachascladproduct zs on zs.zadachascladproduct = zp.zadachascladproduct
inner join scladproduct sp on sp.scladproduct = zs.scladproduct
inner join product p on sp.product = p.product
inner join color c on sp.color = c.color
inner join zadacha z on zn.zadacha = z.zadacha
inner join model m on z.model = m.model
where zn.zadachanastil =  $P{DOC}]]>
	</queryString>
