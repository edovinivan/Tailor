	<parameter name="DOC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
 z.nomer,
 z.dates,
 m.article,
 p.name as product,
 sp.passport,
 c.name as color,
 s.name as sclad,
 sp.ves,
 sp.width
from zadachascladproduct zsc
inner join scladproduct sp on sp.scladproduct = zsc.scladproduct
inner join zadacha z on z.zadacha = zsc.zadacha
inner join model m on m.model = z.model
inner join product p on p.product = sp.product
inner join color c on c.color = sp.color
inner join sclad s on s.sclad = sp.sclad
where zsc.zadacha = $P{DOC}]]>
	</queryString>
