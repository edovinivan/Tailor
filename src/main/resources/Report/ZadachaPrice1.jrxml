	<subDataset name="RabOper" uuid="6783cf42-4538-4ca9-8fa6-14fffcbd66a3">
		<parameter name="zd" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
 zk.nomer,
 coalesce(sum(ro.summaall), 0) as summ
from zadachakomplektovka zk
left join rabotnikoperation ro on zk.zadachakomplektovka = ro.zadachakomplektovka
where zk.zadacha = $P{zd}
group by zk.nomer]]>
		</queryString>
		<field name="NOMER" class="java.lang.String">
			<fieldDescription><![CDATA[РЅРѕРјРµСЂ РєРѕРјРїР»РµРєС‚РѕРІРєРё]]></fieldDescription>
		</field>
		<field name="SUMM" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="ZN" uuid="30fd7cbb-18fc-458a-b5c5-8081540c9451">
		<parameter name="zn" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
  t.name || '(' || zn.nomer || ')' as name,
  (zn.qtygut+zn.qtybeika) as qgut,
  (zn.qtyend+zn.qtydel + zn.qtydel1 +zn.qtylosk+zn.qtytruba) as qdel,
  (zn.qtyaddarticle) as qa,
   zn.qtynull as qtynull,
(select
   sum(sp1.ves)
  from zadachanastilproduct znp1
  inner join zadachascladproduct zsp1 on zsp1.zadachascladproduct = znp1.zadachascladproduct
  inner join scladproduct sp1 on zsp1.scladproduct = sp1.scladproduct
  where znp1.zadachanastil = zn.zadachanastil
) - zn.qtyreturn as qall

/*,
  (case (zn.qtyend+zn.qtydel+zn.qtydel1+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle+zn.qtygut+zn.qtynull) when 0 then 1 else (zn.qtyend+zn.qtydel+zn.qtydel1+zn.qtylosk+zn.qtytruba+zn.qtyaddarticle+zn.qtygut+zn.qtynull) end) as qall*/
from zadachanastil zn
inner join tipnastila t on t.tipnastila = zn.tipnastila
where zn.zadacha = $P{zn}
order by t.name
/*group by t.name*/]]>
		</queryString>
		<field name="NAME" class="java.lang.String"/>
		<field name="QGUT" class="java.math.BigDecimal"/>
		<field name="QDEL" class="java.math.BigDecimal"/>
		<field name="QA" class="java.math.BigDecimal"/>
		<field name="QTYNULL" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[РћСЃС‚Р°С‚РѕС‡РЅС‹Р№ РІРµСЃ]]></fieldDescription>
		</field>
		<field name="QALL" class="java.math.BigDecimal"/>
		<variable name="sumqgut" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QGUT}]]></variableExpression>
		</variable>
		<variable name="allqty" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QALL}]]></variableExpression>
		</variable>
		<variable name="sumqdel" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QDEL}]]></variableExpression>
		</variable>
		<variable name="sumqa" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QA}]]></variableExpression>
		</variable>
		<variable name="sumqtynull" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QTYNULL}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="dopzk" uuid="3e1fd0f7-e754-4f79-9f8b-d0c36836cf41">
		<parameter name="zd2" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
    zk.nomer || ' <' || zk.dates || '><' || (select mm.article from model mm where mm.model = zk.model) || '><' || (select r1.name from rabotnik r1 where r1.rabotnik = zk.rabotnik) || '>' as name,
    (select sum(zz.qty) from zadachakomplektovkarazmeri zz where zz.zadachakomplektovka = zk.zadachakomplektovka) as  qty,
    (case zk.status when 0 then 'Комплектуется' when 1 then 'Комплектуется' when 2 then 'Комплектуется'  when 3 then 'Отпр. в ' || (select name from rabotnikgruppa where rabotnikgruppa = zk.chex) when 4 then 'В цехе '|| (select name from rabotnikgruppa where rabotnikgruppa = zk.chex) when 5 then 'Выполнена в '|| (select name from rabotnikgruppa where rabotnikgruppa = zk.chex) when 6 then 'В оптовом складе' end) as status
  from zadachakomplektovka zk
  where zk.zadacha2 = $P{zd2}]]>
		</queryString>
		<field name="NAME" class="java.lang.String"/>
		<field name="QTY" class="java.lang.Long"/>
		<field name="STATUS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="qtytonastil" uuid="700c371d-85d0-4865-b7dd-7a6793e18dfe">
		<parameter name="zn2" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
 zz.name,
 zz.qty,
 (select
      sum(zr.qty)
  from zadachakomplektovka zk
  inner join zadachakomplektovkarazmeri zr on zr.zadachakomplektovka = zk.zadachakomplektovka
  where zk.zadacha = zz.zadacha
  )
from(
select
 tn.name,
 sum(zn.qtygut+zn.qtybeika) as qty,
 zn.zadacha
from zadachanastil zn
inner join tipnastila tn on tn.tipnastila = zn.tipnastila
where zn.zadacha = $P{zn2}
group by tn.name, zn.zadacha) zz]]>
		</queryString>
		<field name="NAME" class="java.lang.String"/>
		<field name="QTY" class="java.math.BigDecimal"/>
		<field name="SUM" class="java.lang.Long"/>
		<variable name="sum_qty" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QTY}]]></variableExpression>
			<initialValueExpression><![CDATA[$F{QTY}]]></initialValueExpression>
		</variable>
	</subDataset>
