	<queryString>
		<![CDATA[select
   chex,
   gruppa,
   sum(onado) - sum(oest) as otime,
   coalesce((select times from P_TIMEOPCHEX where operationgruppa = opg and chex = rabg),0) as maxt

from(
select
  rg.name as chex,
  og.name as gruppa,
  (select sum(zr.qty * mo.times) from zadachakomplektovkarazmeri zr where zr.zadachakomplektovka = zk.zadachakomplektovka) as onado,
  (select coalesce(sum(ro.qty * ro.timeo),0) from rabotnikoperation ro where ro.zadachakomplektovka = zk.zadachakomplektovka and ro.operation = o.operation) as oest,
  og.operationgruppa as opg,
  rg.rabotnikgruppa as rabg
from zadachakomplektovka zk
inner join rabotnikgruppa rg on rg.rabotnikgruppa = zk.chex
inner join model m on m.model = zk.model
inner join modeloperation mo on mo.model = zk.model
inner join operation o on mo.operation = o.operation
inner join operationgruppa og on og.operationgruppa = o.operationgruppa
where  zk.status between 3 and 4 and zk.chex in (1,2,7))
group by chex, gruppa, opg, rabg
order by chex,   gruppa]]>
	</queryString>
