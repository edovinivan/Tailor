	<queryString>
		<![CDATA[select
  rg.name as chex,
  r.name as rabotname,
  o.name as operation,
  ro.dates,
  ro.summaall,
  ro.qty,
  ro.priceo,
  ro.times,
  ro.pricer,
  ro.timeo,
  m.article,
  (case $P{chex} when 0 then 'Все' else (select name from rabotnikgruppa where rabotnikgruppa = $P{chex}) end) as
 chexname,
(case $P{rabotnik} when 0 then 'Все' else (select name from rabotnik where rabotnik = $P{rabotnik}) end) as
 rabname
from rabotnikoperation ro
inner join rabotnik r on r.rabotnik = ro.rabotnik
inner join zadachakomplektovka zk on zk.zadachakomplektovka = ro.zadachakomplektovka
inner join zadacha z on zk.zadacha = z.zadacha
inner join model m on m.model = z.model
inner join rabotnikgruppa rg on rg.rabotnikgruppa = ro.chex
inner join operation o on o.operation = ro.operation
where ro.chex = (case $P{chex} when 0 then zk.chex else $P{chex} end)
and r.rabotnik = (case $P{rabotnik} when 0 then r.rabotnik else $P{rabotnik} end)
and ro.dates between $P{date1}  and $P{date2}
and zk.chex not in (case (select nochex from operator where operator = $P{oper}) when 0 then -1 else (select chex from OPERATORCHEXSTOP where operator = $P{oper}) end)
order by rg.name, r.name, ro.dates]]>
	</queryString>
