	</parameter>
	<queryString>
		<![CDATA[select
 p.name as product,
 c.name as color,
 sp.price,
 sp.width,
 sp.light,
 sp.ves,
 sp.passport,
 s.name as sclad,
 d.dates,
 a.name as agent
from scladproduct sp
inner join product p on p.product = sp.product and sp.vid = 2
inner join color c on c.color = sp.color
inner join sclad s on s.sclad = sp.sclad
inner join documentjournal dj on dj.documentjournal = sp.documentjournal
inner join document d on dj.document = d.document
inner join agent a on a.agent = d.agent
where sp.status > 0 and sp.scladproduct !=0 and sp.width > 0
and sp.color = (case $P{color} when 0 then sp.color else $P{color} end)
and sp.product = (case $P{product} when 0 then sp.product else $P{product} end)
order by p.name]]>
	</queryString>
