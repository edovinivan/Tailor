	<queryString>
		<![CDATA[select
 (case sp.status when 0 then '!Непр. ' else '' end) || p.name as product,
 c.name as color,
 sp.price,
 sp.width,
 sp.light,
 sp.ves,
 sp.passport,
 s.name as sclad,
 d.dates,
 a.name as agent
from scladproduct sp
inner join product p on p.product = sp.product
inner join color c on c.color = sp.color
inner join sclad s on s.sclad = sp.sclad
inner join documentjournal dj on dj.documentjournal = sp.documentjournal
inner join document d on dj.document = d.document
inner join agent a on a.agent = d.agent
where sp.status !=2 and sp.scladproduct !=0 and sp.vid = 1
order by s.name,p.name]]>
	</queryString>
